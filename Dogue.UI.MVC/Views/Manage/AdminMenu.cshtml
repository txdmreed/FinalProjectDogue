
@{
    ViewBag.Title = "Administrative Menu";
}

<div class="container">
    <h1 class="centerMe">@ViewBag.Title</h1>
    <div class="gContainer">
        <div class="gContent">
            <h2 id="cardTitle">Register</h2>
            <p><a href="@Url.Action("Register", "Account")">Register New User</a></p>
        </div>
    </div>
    <div class="gContainer">
        <div class="gContent">
            <h2 id="cardTitle">Portfolio</h2>

        </div>
